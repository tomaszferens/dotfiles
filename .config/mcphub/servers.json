{
  "mcpServers": {
    "github.com/GLips/Figma-Context-MCP": {
      "command": "npx",
      "args": [
        "-y",
        "figma-developer-mcp",
        "--figma-api-key=${FIGMA_API_KEY}",
        "--stdio"
      ]
    },
    "github.com/modelcontextprotocol/servers/tree/main/src/git": {
      "command": "uvx",
      "args": [
        "mcp-server-git"
      ]
    }
  },
  "nativeMCPServers": {
    "neovim": {
      "disabled": false
    }
  }
}
